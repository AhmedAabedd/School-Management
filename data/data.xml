<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- SEQUENCE FOR STUDENT -->
        <record id="seq_school_student" model="ir.sequence">
            <field name="name">Student Sequence</field>
            <field name="code">school.student.sequence</field>
            <field name="prefix">STU</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- SEQUENCE FOR PARENT -->
        <record id="seq_school_parent" model="ir.sequence">
            <field name="name">Parent Sequence</field>
            <field name="code">school.parent.sequence</field>
            <field name="prefix">PAR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- SEQUENCE FOR TEACHER -->
        <record id="seq_school_teacher" model="ir.sequence">
            <field name="name">Teacher Sequence</field>
            <field name="code">school.teacher.sequence</field>
            <field name="prefix">TEACH</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- SEQUENCE FOR SALE ORDER -->
        <record id="seq_school_sale_order" model="ir.sequence">
            <field name="name">Sale Order Sequence</field>
            <field name="code">school.sale.order.sequence</field>
            <field name="prefix">S</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    
</odoo>